@model IEnumerable<LaCatalanaWeb.Models.AlmacenInventario>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NombreAlmacen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DescripcionAlmacen)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NombreAlmacen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DescripcionAlmacen)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAlmacen">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAlmacen">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAlmacen">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
